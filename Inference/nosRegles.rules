[ruleKnows: (ns:knows rdf:type owl:SymmetricProperty) (?s ns:knows ?o) -> (?o ns:knows ?s)]

[rule1: (frapo:BudgetInformation rdfs:subClassOf frapo:FinancialEntity) (?uri rdf:type ?frapo:BudgetInformation) -> (?uri rdfs:subClassOf frapo:FinancialEntity) ]

[rule2: (dbo:region	 rdfs:subClassOf dbo:PopulatedPlace) (dbo:PopulatedPlace ?rdf:subClassOf ?place) -> ( dbo:region rdfs:subClassOf dbo:Place) ]

[rule3 : (?uri dbo:region ?uri_region) (dbo:region rdfs:subClassOf dbo:Place) -> (?uri dbo:Place ?uri_region) ]

